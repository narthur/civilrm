    "convex": "^1.24.1",
    "next": "^15.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.14",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  }
}
